{
  "definition": {
    "id": "runtime-error-test",
    "version": "1.0.0",
    "name": "Runtime Error Test Process",
    "description": "Test runtime error handling in script_ts",
    "settings": {
      "persistence": "full",
      "timeout": 30000,
      "error_strategy": "stop_and_rollback"
    }
  },
  "trigger": {
    "id": "trg_01",
    "type": "http_webhook",
    "config": {
      "path": "/v1/runtime-error",
      "method": "POST"
    }
  },
  "nodes": [
    {
      "id": "runtime_error_script",
      "type": "script_ts",
      "description": "Script with runtime error",
      "input_mapping": {
        "value": "$.trigger.body.value"
      },
      "script": "const obj = null; const result = obj.property; result;"
    }
  ],
  "transitions": []
}
