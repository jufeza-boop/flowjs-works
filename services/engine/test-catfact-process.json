{
  "definition": {
    "id": "catfact-get-process",
    "version": "1.0.0",
    "name": "CatFact GET Process",
    "description": "Integration flow that performs a GET request to catfact.ninja",
    "settings": {
      "persistence": "full",
      "timeout": 30000,
      "error_strategy": "stop_and_rollback"
    }
  },
  "trigger": {
    "id": "trg_01",
    "type": "http_webhook",
    "config": {
      "path": "/v1/catfact",
      "method": "GET"
    }
  },
  "nodes": [
    {
      "id": "get_cat_fact",
      "type": "http",
      "description": "GET cat fact from public API",
      "config": {
        "url": "https://catfact.ninja/fact",
        "method": "GET",
        "timeout": 30,
        "headers": {
          "authority": "catfact.ninja",
          "accept": "application/json",
          "accept-language": "es-ES,es;q=0.9,en;q=0.8",
          "cookie": "XSRF-%3D; catfacts_%3D; ",
          "referer": "https://catfact.ninja/",
          "sec-ch-ua": "\"Chromium\";v=\"122\", \"Not(A:Brand\";v=\"24\", \"Google Chrome\";v=\"122\"",
          "sec-ch-ua-mobile": "?0",
          "sec-ch-ua-platform": "\"Windows\"",
          "sec-fetch-dest": "empty",
          "sec-fetch-mode": "cors",
          "sec-fetch-site": "same-origin",
          "user-agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36",
          "x-csrf-token": "BnfqU78kXNtpirMm0GXsgmrGHrA8jOwOIgslpCDX"
        }
      }
    }
  ],
  "transitions": []
}