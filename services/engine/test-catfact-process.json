{
  "definition": {
    "id": "catfact-get-process",
    "version": "1.0.0",
    "name": "CatFact GET Process",
    "description": "Integration flow that performs a GET request to catfact.ninja",
    "settings": {
      "persistence": "full",
      "timeout": 30000,
      "error_strategy": "stop_and_rollback"
    }
  },
  "trigger": {
    "id": "trg_01",
    "type": "http_webhook",
    "config": {
      "path": "/v1/catfact",
      "method": "GET"
    }
  },
  "nodes": [
    {
      "id": "get_cat_fact",
      "type": "http",
      "description": "GET cat fact from public API",
      "config": {
        "url": "https://fakerestapi.azurewebsites.net/api/v1/Activities",
        "method": "GET",
        "timeout": 30      
      }
    }
  ],
  "transitions": []
}