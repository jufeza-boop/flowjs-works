{
  "definition": {
    "id": "complex-script-test",
    "version": "1.0.0",
    "name": "Complex Script Test Process",
    "description": "Test complex transformations with script_ts",
    "settings": {
      "persistence": "full",
      "timeout": 30000,
      "error_strategy": "stop_and_rollback"
    }
  },
  "trigger": {
    "id": "trg_01",
    "type": "http_webhook",
    "config": {
      "path": "/v1/complex-test",
      "method": "POST"
    }
  },
  "nodes": [
    {
      "id": "complex_transform",
      "type": "script_ts",
      "description": "Complex data transformation",
      "input_mapping": {
        "users": "$.trigger.body.users",
        "companyName": "$.trigger.body.company"
      },
      "script": "const adults = input.users.filter(u => u.age >= 18); const output = { company: input.companyName, adultCount: adults.length, adultNames: adults.map(u => u.name), summary: input.companyName + ' has ' + adults.length + ' adult users' }; output;"
    },
    {
      "id": "log_result",
      "type": "logger",
      "description": "Log the transformation result",
      "input_mapping": {
        "message": "$.nodes.complex_transform.output"
      },
      "config": {
        "level": "info"
      }
    }
  ],
  "transitions": []
}
